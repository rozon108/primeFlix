<% recievedList.forEach( movie=>{ %>

    
      <div class="box-img">
        <img src="<%= movie.poster %>" />
      </div>
     
        <div class="movie-info ">
          <span class="text-muted"><%= movie.year%></span>
          <span class="float-end"><%= movie.imdb.rating%> ⭐️</span>
        </div>
      
    
<% }) %>

